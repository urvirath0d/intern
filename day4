u=list([1, 2, 3, 4, 5, 6])
u.append(7)
print(u)
del u[2]
print(u)
del u[0:2]
print(u)
A = list([15, 67, 34, 89, 1])
largest = max(A)
print(largest)
smallest = min(A)
print(smallest)
x = (1, 2, 3, 4, 5)
rev1 = x[::-1]
print(rev1)
E=('my', 'name', 'is', 'urvi')
print(E)
convert=list(E)
print((convert))
